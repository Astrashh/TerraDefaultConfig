id "mineshaft_start";

num layers = randomInt(2) + 4;

for(num i = 0; i < layers; i = i + 1) {
    structure(0, i * 5, 0, "mineshaft_section", "NONE");
    structure(0, i * 5, 0, "mineshaft_section", "CW_90");
    structure(0, i * 5, 0, "mineshaft_section", "CCW_90");
    structure(0, i * 5, 0, "mineshaft_section", "CW_180");
}